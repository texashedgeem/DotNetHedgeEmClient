	<log4net>
		<root>
			<level value="INFO"/>
			<appender-ref ref="LogFileAppender"/>
			<appender-ref ref="log_HedgeEmServer"/>
			<appender-ref ref="log_HedgeEmTable"/>
			<appender-ref ref="log_HedgeEmStatsEventRecorder"/>
			<appender-ref ref="log_HedgeEmStats_zzz"/>
		</root>
		<logger name="HedgeEmServer">
			<level value="DEBUG"/>
			<appender-ref ref="log_HedgeEmServer"/>
		</logger>
		<logger name="TexasHedgeEm.HedgeEMTable">
			<level value="DEBUG"/>
			<appender-ref ref="log_HedgeEmTable"/>
		</logger>
		<logger name="HedgeEmStatsApp.HedgeEmStatsEventRecorder">
			<level value="DEBUG"/>
			<appender-ref ref="log_HedgeEmStatsEventRecorder"/>
		</logger>
		<logger name="HedgeEmStatsApp.frm_stats_console">
			<level value="DEBUG"/>
			<appender-ref ref="log_HedgeEmStats_zzz"/>
		</logger>
		<appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
			<param name="File" value="C:\temp\logs\log4net_default.log"/>
			<!--<param name="File" value="temp/log4net_default.log"/>-->
			<param name="AppendToFile" value="true"/>
			<rollingStyle value="Size"/>
			<maxSizeRollBackups value="10"/>
			<maximumFileSize value="10MB"/>
			<staticLogFileName value="true"/>
			<layout type="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" value="%-6p%d{yyyy-MM-dd hh:mm:ss} – %m%n"/>
			</layout>
		</appender>
		<appender name="log_HedgeEmServer" type="log4net.Appender.RollingFileAppender">
			<param name="File" value="C:\temp\logs\log4net_hedgeem_server.log"/>
			<!--<param name="File" value="temp/log4net_hedgeem_server.log"/>-->
			<param name="AppendToFile" value="true"/>
			<rollingStyle value="Size"/>
			<maxSizeRollBackups value="10"/>
			<maximumFileSize value="10MB"/>
			<staticLogFileName value="true"/>
			<layout type="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" value="%-6p%d{yyyy-MM-dd hh:mm:ss} – %m%n"/>
			</layout>
		</appender>
		<appender name="log_HedgeEmTable" type="log4net.Appender.RollingFileAppender">
			<param name="File" value="C:\temp\logs\log4net_hedgeem_table.log"/>
			<!--<param name="File" value="temp/log4net_hedgeem_table.log"/>-->
			<param name="AppendToFile" value="true"/>
			<rollingStyle value="Size"/>
			<maxSizeRollBackups value="10"/>
			<maximumFileSize value="10MB"/>
			<staticLogFileName value="true"/>
			<layout type="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" value="%-6p%d{yyyy-MM-dd hh:mm:ss} – %m%n"/>
			</layout>
		</appender>
		<appender name="log_HedgeEmStatsEventRecorder" type="log4net.Appender.RollingFileAppender">
			<param name="File" value="C:\temp\logs\log4net_hedgeem_HedgeEmStatsEventRecorder.log"/>
			<!--<param name="File" value="temp/log4net_hedgeem_HedgeEmStatsEventRecorder.log"/>-->
			<param name="AppendToFile" value="true"/>
			<rollingStyle value="Size"/>
			<maxSizeRollBackups value="10"/>
			<maximumFileSize value="10MB"/>
			<staticLogFileName value="true"/>
			<layout type="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" value="%-6p%d{yyyy-MM-dd hh:mm:ss} – %m%n"/>
			</layout>
		</appender>
		<appender name="log_HedgeEmStats_zzz" type="log4net.Appender.RollingFileAppender">
			<param name="File" value="C:\temp\logs\log4net_hedgeem_stats_zzz.log"/>
			<!--<param name="File" value="temp/log4net_hedgeem_stats_zzz.log"/>-->
			<param name="AppendToFile" value="true"/>
			<rollingStyle value="Size"/>
			<maxSizeRollBackups value="10"/>
			<maximumFileSize value="10MB"/>
			<staticLogFileName value="true"/>
			<layout type="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" value="%-6p%d{yyyy-MM-dd hh:mm:ss} – %m%n"/>
			</layout>
		</appender>
	</log4net>
